{
  "name": "chicago-housing-helper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter web dev",
    "build": "pnpm --filter web build",
    "lint": "pnpm -r lint",
    "db:generate": "pnpm --filter @chicago-housing-helper/database generate",
    "db:push": "pnpm --filter @chicago-housing-helper/database push",
    "db:seed": "pnpm --filter @chicago-housing-helper/database seed",
    "db:studio": "pnpm --filter @chicago-housing-helper/database studio"
  },
  "devDependencies": {
    "typescript": "^5.6.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    }
  }
}